<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Define rules for what data can be backed up to the cloud -->
        <exclude domain="sharedpref" path="." />
        <exclude domain="database" path="." />
        <exclude domain="file" path="." />
        <exclude domain="external" path="." />
    </cloud-backup>
    <device-transfer>
        <!-- Define rules for what data can be transferred to a new device -->
        <exclude domain="sharedpref" path="." />
        <exclude domain="database" path="." />
        <exclude domain="file" path="." />
        <exclude domain="external" path="." />
    </device-transfer>
</data-extraction-rules>