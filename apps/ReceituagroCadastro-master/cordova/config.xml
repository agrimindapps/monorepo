<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.agrimind.pragassoja" android-versionCode="20211009" ios-CFBundleVersion="1" version="2021.10.09" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>receituagro</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <!-- this hook will point your config.xml to the DevServer on Serve -->
    <hook type="after_prepare" src="../node_modules/vue-cli-plugin-cordova/serve-config-hook.js" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>

    <preference name="cordova-version" value="cli-10.0.0" />

    <preference name="StatusBarBackgroundColor" value="#f2f2f2" />
    <preference name="StatusBarStyle" value="default" />

    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="SplashScreenBackgroundColor" value="green"/>
    <preference name="SplashMaintainAspectRatio" value="true" />

    <preference name="DisallowOverscroll" value="true" />

    <platform name="android">
        <icon platform="android" qualifier="xhdpi" src="../public/icon/android/xhdpi.png" />
        <icon platform="android" qualifier="ldpi" src="../public/icon/android/ldpi.png" />
        <icon platform="android" qualifier="mdpi" src="../public/icon/android/mdpi.png" />
        <icon platform="android" qualifier="hdpi" src="../public/icon/android/hdpi.png" />
        <icon platform="android" qualifier="xxhdpi" src="../public/icon/android/xxhdpi.png" />
        <icon platform="android" qualifier="xxxhdpi" src="../public/icon/android/xxxhdpi.png" />
    </platform>
    <platform name="ios">
         <!-- iTunes Marketing Image -->
        <icon height="1024" src="../public/icon/ios/iTunesArtwork@2x.png" width="1024" />
        <!-- iPad Pro -->
        <icon height="167" src="../public/icon/ios/Icon-83.5@2x.png" width="167" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon height="100" src="../public/icon/ios/Icon-Small-50@2x.png" width="100" />
        <icon height="50" src="../public/icon/ios/Icon-Small-50.png" width="50" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon height="87" src="../public/icon/ios/Icon-Small@3x.png" width="87" />
        <icon height="58" src="../public/icon/ios/Icon-Small@2x.png" width="58" />
        <icon height="29" src="../public/icon/ios/Icon-Small.png" width="29" />
        <!-- iPad -->
        <icon height="144" src="../public/icon/ios/Icon-72@2x.png" width="144" />
        <icon height="72" src="../public/icon/ios/Icon-72.png" width="72" />
        <!-- iOS 6.1 -->
        <!-- iPhone / iPod Touch -->
        <icon height="114" src="../public/icon/ios/Icon@2x.png" width="114" />
        <!-- Spotlight Icon -->
        <icon height="80" src="../public/icon/ios/Icon-Small-40@2x.png" width="80" />
        <icon height="40" src="../public/icon/ios/Icon-Small-40.png" width="40" />
        <!-- iPad -->
        <icon height="152" src="../public/icon/ios/Icon-76@2x.png" width="152" />
        <icon height="76" src="../public/icon/ios/Icon-76.png" width="76" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon height="120" src="../public/icon/ios/Icon-60@2x.png" width="120" />
        <icon height="60" src="../public/icon/ios/Icon-Notification@3x.png" width="60" />
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon height="180" src="../public/icon/ios/Icon-60@3x.png" width="180" />
    </platform>
    <platform name="android">
        <splash density="land-xxxhdpi" src="../public/screen/android/splash-land-xxxhdpi.png" />
        <splash density="land-xxhdpi" src="../public/screen/android/splash-land-xxhdpi.png" />
        <splash density="land-xhdpi" src="../public/screen/android/splash-land-xhdpi.png" />
        <splash density="land-hdpi" src="../public/screen/android/splash-land-hdpi.png" />
        <splash density="land-mdpi" src="../public/screen/android/splash-land-mdpi.png" />
        <splash density="land-ldpi" src="../public/screen/android/splash-land-ldpi.png" />
    </platform>
    <platform name="android">
        <splash density="port-xxxhdpi" src="../public/screen/android/splash-port-xxxhdpi.png" />
        <splash density="port-xxhdpi" src="../public/screen/android/splash-port-xxhdpi.png" />
        <splash density="port-xhdpi" src="../public/screen/android/splash-port-xhdpi.png" />
        <splash density="port-hdpi" src="../public/screen/android/splash-port-hdpi.png" />
        <splash density="port-mdpi" src="../public/screen/android/splash-port-mdpi.png" />
        <splash density="port-ldpi" src="../public/screen/android/splash-port-ldpi.png" />
    </platform>
    <platform name="ios">
        <splash src="../public/screen/ios/Default@2x~iphone~anyany.png" />
        <splash src="../public/screen/ios/Default@2x~iphone~comany.png" />
        <splash src="../public/screen/ios/Default@2x~iphone~comcom.png" />
        <splash src="../public/screen/ios/Default@3x~iphone~anyany.png" />
        <splash src="../public/screen/ios/Default@3x~iphone~anycom.png" />
        <splash src="../public/screen/ios/Default@3x~iphone~comany.png" />
        <splash src="../public/screen/ios/Default@2x~ipad~anyany.png" />
        <splash src="../public/screen/ios/Default@2x~ipad~comany.png" />
    </platform>

    <platform name="android">
        <preference name="android-build-tool" value="gradle" />
        <preference name="android-windowSoftInputMode" value="adjustResize" />
        <preference name="android-minSdkVersion" value="24" />
        <preference name="android-targetSdkVersion" value="30" />
        <preference name="android-installLocation" value="preferInternal" />
        <preference name="AndroidPersistentFileLocation" value="Compatibility" />
        <preference name="AndroidXEnabled" value="true" />
        <preference name="GradlePluginKotlinEnabled" value="true" />
        <preference name="GradlePluginKotlinCodeStyle" value="official" />
        <preference name="GradlePluginKotlinVersion" value="1.3.50" />
    </platform>

    <platform name="ios">
        <preference name="target-device" value="universal" />
        <preference name="prerendered-icon" value="true" />
        <preference name="detect-data-types" value="true" />
        <preference name="exit-on-suspend" value="false" />
        <preference name="deployment-target" value="12.0" />
        <preference name="SwiftVersion" value="5.3" />
    </platform>
</widget>
